{% extends 'base.html.twig' %}

{% block title %}Annee index{% endblock %}

{% block body %}
    <h1>Annee index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>Year_start</th>
                <th>YearEnd</th>
                <th>YearStatut</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for annee in annees %}
            <tr>
                <td>{{ annee.id }}</td>
                <td>{{ annee.yearStart ? annee.yearStart|date('Y-m-d') : '' }}</td>
                <td>{{ annee.yearEnd ? annee.yearEnd|date('Y-m-d') : '' }}</td>
                <td>{{ annee.yearStatut ? 'Yes' : 'No' }}</td>
                <td>
                    <a href="{{ path('app_annee_show', {'id': annee.id}) }}">show</a>
                    <a href="{{ path('app_annee_edit', {'id': annee.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="5">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('app_annee_new') }}">Create new</a>
{% endblock %}
