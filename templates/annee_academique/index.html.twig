{% extends 'base.html.twig' %}

{% block title %}AnneeAcademique index{% endblock %}

{% block body %}
    <h1>AnneeAcademique index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>YearStart</th>
                <th>YearEnd</th>
                <th>IsCurrent</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for annee_academique in annee_academiques %}
            <tr>
                <td>{{ annee_academique.id }}</td>
                <td>{{ annee_academique.YearStart ? annee_academique.YearStart|date('Y-m-d') : '' }}</td>
                <td>{{ annee_academique.YearEnd ? annee_academique.YearEnd|date('Y-m-d') : '' }}</td>
                <td>{{ annee_academique.isCurrent ? 'Yes' : 'No' }}</td>
                <td>
                    <a href="{{ path('app_annee_academique_show', {'id': annee_academique.id}) }}">show</a>
                    <a href="{{ path('app_annee_academique_edit', {'id': annee_academique.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="5">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('app_annee_academique_new') }}">Create new</a>
{% endblock %}
