{% extends 'base.html.twig' %}

{% block title %}Evaluation{% endblock %}

{% block body %}
    <h1>Evaluation</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ evaluation.id }}</td>
            </tr>
            <tr>
                <th>DateDebut</th>
                <td>{{ evaluation.dateDebut ? evaluation.dateDebut|date('Y-m-d') : '' }}</td>
            </tr>
            <tr>
                <th>TempsEvaluation</th>
                <td>{{ evaluation.tempsEvaluation }}</td>
            </tr>
            <tr>
                <th>Statut</th>
                <td>{{ evaluation.statut }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_evaluation_index') }}">back to list</a>

    <a href="{{ path('app_evaluation_edit', {'id': evaluation.id}) }}">edit</a>

    {{ include('evaluation/_delete_form.html.twig') }}
{% endblock %}
